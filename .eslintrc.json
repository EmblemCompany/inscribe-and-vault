{
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 13
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended",
    "@vue/eslint-config-prettier"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/valid-v-slot": "off",
    "vue/no-v-model-argument": "off",
    "vue/no-multiple-template-root": "off"
  }
}